{
  "productName": "fotobox",
  "directories": {
    "output": "dist/builder-output"
  },
  "asarUnpack": [
    "src/main/**/*.jpg",
    "src/main/**/*.png"
  ],
  "files": [
    "src/main/**/*",
    "!*.ts",
    "!*.map"
  ],
  "win": {
    "icon": "dist/renderer/assets/icon",
    "target": [
      "nsis"
    ]
  },
  "mac": {
    "icon": "dist/renderer/assets/icon",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/renderer/assets/icon",
    "target": [
      "AppImage"
    ]
  }
}
