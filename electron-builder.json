{
  "productName": "fotobox",
  "directories": {
    "app": "dist",
    "output": "dist/builder-output"
  },
  "asarUnpack": [
    "**/*.jpg",
    "**/*.png"
  ],
  "files": [
    "**/*",
    "!*.map",
    "!builder-output"
  ],
  "win": {
    "icon": "dist/renderer/assets/icon/icon.ico",
    "target": [
      "nsis"
    ]
  },
  "mac": {
    "icon": "dist/renderer/assets/icon/icon.ico",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/renderer/assets/icon/icon.ico",
    "target": [
      "AppImage"
    ]
  }
}
